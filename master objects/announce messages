weapon:announce_parry(target,attacker,weapon)dobj = args[2];player = args[1];iobj = args[3];$you:say_action(this.parry_msg);armor:announce_soak(target,attacker,weapon,body_area);dobj = args[2];player = args[1];iobj = args[3];$you:say_action(this.soak_msg);player:announce_dodge(target,attacker,weapon);dobj = args[2];player = args[1];iobj = args[3];$you:say_action((this.species).dodge_msg);player:announce_hit(target,attacker,weapon,body_area,attack);dobj = args[2];player = args[1];iobj = args[3];$you:say_action(this.species:get_damage_message(args[5].damage_type,args[4]),player,iobj,args[4]);attack:announce_attack(target,attacker,weapon,body_area)dobj = args[2];player = args[1];iobj = args[3];$you:say_action(this.attack_msg,player,iobj,args[4]);generic weapon message set.oparry_msg.parry_msg.oattack_msg.attack_msg.soak_msg.osoak_msg.dodge_msg.ododge_msg