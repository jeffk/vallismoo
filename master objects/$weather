$weather:get_weather($zone,coordinates)-------------------------------":get_weather($zone,coordinates)";"Returns the weather for the given $zone and coordinates.";"Returns one of: rain/snow/clear/clouds/fog as a string.";return ("clear");:get_season($zone)-------------------------------":get_season($zone)";"Returns the season for the given $zone.";month = $time_utils:time_sub("$n",$rpg:ic_time());if ((month == "Dec") || (month == "Jan") || (month == "Feb"))  return("winter");elseif ((month == "Mar") || (month == "Apr") || (month == "May"))  return("spring");elseif ((month == "Jun") || (month == "Jul") || (month == "Aug"))  return("summer");else  return("fall");endif:get_time($zone)-------------------------------":get_time($zone)";"Returns the time for the given $zone.";"Returns one of: morning/day/evening/night as a string.";time = tonum($time_utils:time_sub("$H",$rpg:ic_time()));if (((time >= 0) && (time <7)) || (time > 21))  return("night");elseif ((time >= 7) && (time <10))  return("morning");elseif ((time >= 10) && (time <18))  return("day");else  return("evening");endif