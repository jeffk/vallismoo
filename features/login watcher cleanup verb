property = this.watchers;counter = 1;for prop in (property)  $command_utils:suspend_if_needed(1);  for entry in (prop[2])    if (entry != "all")      if ((!valid(entry)) || (!$object_utils:isa(entry,$player)))        property[counter][2] = setremove(property[counter][2],entry);      endif    endif  endfor  counter = counter + 1;endforthis.watchers = property;for prop in (property)  $command_utils:Suspend_if_needed(1);  if ((!valid(prop[1])) || (!$object_utils:isa(prop[1],$player)))    this.watchers = setremove(this.watchers,prop);  endifendforplayer:tell("Done.");