if (!$rpg:trusted(player))  player:tell("You can't do that!");  return;endiftarget = $string_utils:match_player(args[1]);if (target == #-1)  player:tell("Not found!");  return;elseif (target == #-2)  player:tell("Not found!");  return;elseif (target == #-3)  player:tell("Not found!");  return;endifif (typeof(target.owned_objects) != LIST)  player:tell("Erp, their .owned_objects property isn't a list!");  return;endifitemlist = {};for item in (target.owned_objects)  $command_utils:suspend_if_needed(1);  if ($object_utils:has_property(item,"object_size"))    if (item.object_size[1] >= 2500)      itemlist = #1010:flatten({{item},itemlist});    endif  endifendforitemlist = #1010:flatten(itemlist);finallist = {};while (length(itemlist) != 0)  $command_utils:suspend_if_needed(1);  size = 0;  for item in (itemlist)    if (item.object_size[1] >= size)      size = item.object_size[1];      obj = item;    endif  endfor  finallist = #1010:flatten({finallist,{obj}});  itemlist = setremove(itemlist,obj);endwhilefinallist = #1010:flatten(finallist);if (length(finallist) > 20)  finallist = finallist[1..20];endiffor thing in (finallist)  player:tell("Object: ",#9016:left(tostr(thing.name," (",thing,")"),55)," Size: ",#411:byte_str(thing.object_size[1]));endfor;